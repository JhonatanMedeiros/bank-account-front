    "@angular/router": "~8.1.2",
    "angular-bootstrap-md": "^8.3.1",
    "core-js": "^3.3.3",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.13",
